    var c = document.querySelector('[aria-label="Tweet"]')
	c.click();
    await sleep({ seconds: 1 });

	let event = await document.createEvent("UIEvents");
    await sleep({ seconds: 1 });

	var select = await document.querySelector('.public-DraftStyleDefault-block, .public-DraftStyleDefault-ltr')
